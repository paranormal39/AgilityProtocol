{
  "name": "agility-headless",
  "version": "1.0.0",
  "description": "Headless Identity Wallet Engine for orchestrating private identity proofs using Midnight and public settlement using XRPL",
  "type": "module",
  "main": "dist/AgilityHeadless.js",
  "types": "dist/AgilityHeadless.d.ts",
  "exports": {
    ".": {
      "import": "./dist/AgilityHeadless.js",
      "types": "./dist/AgilityHeadless.d.ts"
    }
  },
  "bin": {
    "agility": "./dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "cli": "node dist/cli.js",
    "dev:cli": "npx tsx src/cli.ts",
    "prepublishOnly": "npm run build"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "keywords": [
    "identity",
    "wallet",
    "xrpl",
    "midnight",
    "did",
    "verifiable-credentials",
    "zero-knowledge-proofs"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "dotenv": "^16.3.1",
    "xrpl": "^3.0.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.0"
  }
}
